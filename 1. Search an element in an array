CC_POTD 1   --->   https://www.codechef.com/practice/course/arrays/ARRAYS/problems/SEARCHINARR

import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
    public static void main(String[] args) throws java.lang.Exception
    {
        // your code goes here
        Scanner sc = new Scanner(System.in);
        boolean ans = false;
        int n = sc.nextInt();
        int x = sc.nextInt();
        int a[] = new int[n];
        for (int i = 0; i < n; i++) {
            a[i] = sc.nextInt();
        }
        for (int i = 0; i < n; i++) {
            if (a[i] == x) {
                ans = true;
                break;
            }
        }

        if (ans) {
            System.out.println("yes");
        }
        else {
            System.out.println("no");

        }
    }
}
